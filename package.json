{
  "name": "@votingworks/hmpb-interpreter",
  "version": "3.1.2",
  "description": "Interprets hand-marked paper ballots.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/votingworks/hmpb-interpreter",
  "author": "Brian Donovan <brian@voting.works>",
  "license": "GPL-3.0",
  "scripts": {
    "prepare": "tsc",
    "lint": "eslint '{src,test}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist",
    "!*.test.{js,d.ts}",
    "!dist/test"
  ],
  "bin": {
    "hmpb": "bin/hmpb-interpreter.js",
    "hmpb-interpreter": "bin/hmpb-interpreter.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --quiet --fix"
    ],
    "*.+(css|graphql|json|less|md|mdx|sass|scss|yaml|yml)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.1",
    "@types/jsfeat": "./types/jsfeat",
    "@types/node": "^13.11.1",
    "@types/table": "^5.0.0",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "sort-package-json": "^1.41.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@votingworks/ballot-encoder": "^1.2.0",
    "canvas": "^2.6.1",
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "jsfeat": "^0.0.8",
    "jsqr": "^1.3.0",
    "node-quirc": "^2.2.1",
    "table": "^5.4.6",
    "uuid": "^7.0.3"
  }
}
